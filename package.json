{
    "name": "@fhirschema/codegen",
    "version": "0.0.9",
    "author": "niquola",
    "license": "ISC",
    "main": "dist/fhirschema-codegen.cjs",
    "module": "dist/fhirschema-codegen.js",
    "types": "dist/index.d.ts",
    "engines": {
        "node": ">=20.0.0"
    },
    "bin": {
        "fscg": "./dist/cli.js",
        "fhirschema-codegen": "./dist/cli.js"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "package.json",
        "docs/**"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fhir-schema/fhir-schema-codegen.git"
    },
    "bugs": {
        "url": "https://github.com/fhir-schema/fhir-schema-codegen/issues"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/fhirschema-codegen.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/fhirschema-codegen.cjs"
            }
        }
    },
    "scripts": {
        "build": "vite build && npm run copy-static",
        "copy-static": "copyfiles -u 2 \"src/generators/*/static/**/*\" dist/generators && copyfiles -u 2 \"src/commands/static/**/*\" dist/commands",
        "start": "node dist/cli.js",
        "dev": "tsc -w",
        "test": "vitest run",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest run --coverage",
        "lint": "biome lint src",
        "format": "biome format src",
        "check": "biome check src",
        "check:fix": "biome check --fix src"
    },
    "keywords": [
        "fhir",
        "fhirschema",
        "codegen",
        "typescript",
        "javascript"
    ],
    "description": "Code generator for FHIR Schema",
    "devDependencies": {
        "@biomejs/biome": "^2.0.0-beta.1",
        "@type-challenges/utils": "^0.1.1",
        "@types/node": "^22.8.7",
        "@vitest/coverage-v8": "^3.0.9",
        "copyfiles": "^2.4.1",
        "tsx": "^4.19.2",
        "typescript": "^5.6.3",
        "vite": "^6.2.4",
        "vite-plugin-dts": "^4.5.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.9"
    },
    "dependencies": {
        "commander": "13.1.0",
        "picocolors": "^1.1.1"
    },
    "publishConfig": {
        "access": "public"
    }
}
